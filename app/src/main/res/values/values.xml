<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="backgroundColor">#FFFFFF</color>
    <color name="colorAccent">#03DAC5</color>
    <color name="colorPrimary">#6200EE</color>
    <color name="colorPrimaryDark">#3700B3</color>
    <color name="color_grey">#999999</color>
    <color name="color_grey1">#bbbbbb</color>
    <color name="color_grey2">#dfdfdf</color>
    <color name="color_text">#000000</color>
    <declare-styleable name="Picker">
        <attr name="offset" format="integer" />
        <attr name="darkModeEnabled" format="boolean" />
        <attr name="textSize" format="integer" />
        <attr name="pickerMode" format="enum">
            <enum name="monthFirst" value="0" />
            <enum name="dayFirst" value="1" />
        </attr>
    </declare-styleable>
</resources>